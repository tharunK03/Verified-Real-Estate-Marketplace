{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug with Nodemon",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "nodemon",
        "program": "${workspaceFolder}/server/server.js",
        "restart": true,
        "console": "integratedTerminal",
        "env": {
          "PORT": "5000",
          "MONGO_URI": "mongodb://localhost:27017/real_estate",
          "JWT_SECRET": "your_secret_key"
        }
      }
    ]
  }
  